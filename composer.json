{
  "name": "mahmudkuet11/nova-multiselect-field",
  "description": "A multiple select field for Laravel Nova.",
  "keywords": [
    "laravel",
    "nova",
    "multiselect",
    "select"
  ],
  "authors": [
    {
      "name": "MD. Mahmud Ur Rahman",
      "email": "mahmudkuet11@gmail.com",
      "role": "Maintainer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0"
  },
  "autoload": {
    "psr-4": {
      "Mahmud\\MultiselectField\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mahmud\\MultiselectField\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
